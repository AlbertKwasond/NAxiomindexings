@model AxiomAdmin.ViewModel.VolunteersViewModel
@foreach (var item in Model.ListVolunteers)
{
    <div class="col">
        <div class="card h-100">
            <img class="card-img-top" src="@Url.Content("https://img.axiomindexing.com/assets/img/volunteers/"+item.ImageUrl)" alt="Card image cap" height="466" />
            <div class="card-body">
                <h5 class="card-title">
                    @Html.DisplayFor(model => item.Name)
                </h5>

                <a href="#" data-id="@item.Id" class="card-link edit-item-btn">Edit</a>
                <a asp-action="Delete" asp-controller="Volunteers" asp-route-id="@item.Id" class="card-link">Delete</a>
            </div>
        </div>
    </div>
}
